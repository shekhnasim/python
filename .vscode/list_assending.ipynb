   "source": [
    "#l1=[2,5,3,6,8,4,1]\n",
    "#Short in Assending order\n",
    "ls=[2,5,7,3,6,8,4,1]\n",
    "n=len(ls)-1\n",
    "for j in range(0,n):\n",
    "    for i in range(0,n):\n",
    "        print('|',end='')\n",
    "        if ls[i]>ls[i+1]:\n",
    "            ls[i],ls[i+1]=ls[i+1],ls[i]\n",
    "    n=n-1\n",
    "ls"
   ]
  }
