    "#Word Count\n",
    "string = 'This this is a a, sentense.'\n",
    "counter = {}\n",
    "words = string.split(' ')\n",
    "for word in words:\n",
    "    word = word.lower()\n",
    "    word = word.replace('.','').replace(',','')\n",
    "    if word in counter.keys():\n",
    "        counter[word] +=1\n",
    "    else:\n",
    "        counter[word] = 1\n",
    "counter"
